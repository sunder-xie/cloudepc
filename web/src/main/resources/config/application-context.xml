<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <!-- 扫描文件 -->
    <context:component-scan base-package="com.tqmall.data.epc"/>

    <!--读取配置文件-->
    <import resource="properties.xml"/>

    <!--监控-->
    <import resource="application-monitor.xml" />

    <import resource="application-dubbo.xml" />

    <import resource="application-redis.xml" />

    <import resource="application-tool.xml" />

    <import resource="mybatis-config.xml" />

    <import resource="shiro-config.xml" />

    <import resource="application-transaction.xml"/>

    <import resource="application-aop.xml" />

</beans>
